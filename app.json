{
  "expo": {
    "name": "Atarefado",
    "slug": "Atarefado",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ribeirinho64x.gerenciadortask"
    },
    "description": "gerenciador de tarefas com firebase feito em react native",
    "githubUrl": "https://github.com/caio64x/gerenciador_task"
  }
}
